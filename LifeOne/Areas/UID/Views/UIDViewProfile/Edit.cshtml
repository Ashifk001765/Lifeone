@model LifeOne.Models.AssociateManagement.AssociateEntity.AssociateProfile
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_UIDLayout.cshtml";
}

<style>
    label {
        font-weight: 500;
    }

    .formtab {
        display: none;
        max-width: 1050px;
        width: 100%;
        margin: 0 auto;
    }

    .form-horizontal .formtab .col-md-5.control-label {
        max-width: 200px;
        width: 100%;
    }

    .form-horizontal .formtab .col-md-7 {
        width: calc(100% - 200px);
    }

    fieldset {
        min-width: 0;
        padding: 15px;
        margin: 0 0 20px 0;
        border: 1px solid silver;
    }

    legend {
        display: block;
        width: auto;
        padding: 0 5px;
        margin-bottom: 10px;
        font-size: 18px;
        line-height: inherit;
        color: #333;
        border: 0;
        border-bottom: 0;
    }
    /* Make circles that indicate the steps of the form: */
    .step {
        height: 15px;
        width: 15px;
        margin: 0 2px;
        background-color: #bbbbbb;
        border: none;
        border-radius: 50%;
        display: inline-block;
        opacity: 0.5;
    }

        .step.active {
            opacity: 1;
        }

        /* Mark the steps that are finished and valid: */
        .step.finish {
            background-color: #4CAF50;
        }
</style>
<link href="~/Content/dist/css/sweetalert.css" rel="stylesheet" />
<div class="content">

    <!-- Start Content-->
    <div class="container-fluid">

        <div class="row page-titles">
            <div class="ptitle">
                <h4 class="my-3">User Application Form</h4>
            </div>
        </div>
        <div class="row">

            <!-- end col -->
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <form id="regForm" action="#">
                            <div class="formtab">
                                <div class="row">
                                    <div class="col-md-12 mb-15">
                                        <fieldset>
                                            <legend>Step 1: PERSONAL DETAILS</legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Sponseror ID<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @*<input type="text" class="form-control" placeholder="POLYRICH2">*@
                                                            @Html.TextBoxFor(Model => Model.InviteCode, new { @class = "form-control", @readonly = "readonly", @onchange = "GetReferDetails()", @placeholder = "InviteCode" })
                                                            @Html.HiddenFor(Model => Model.Fk_SponserId)
                                                            @Html.HiddenFor(Model => Model.Fk_MemId)
                                                        </div>
                                                        <div class="col-md-5"></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Sponseror Name:<span class="text-danger">*</span></label>
                                                        <div class="col-md-4 pt-10"> <strong class="text-green" id="sponsorName">@Html.DisplayFor(Model => Model.SponserName)</strong> </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Account Purpose<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="custom_radio">
                                                                <input type="radio" id="Farmer" name="AccountPurpose" value="Farmer">
                                                                <label for="Farmer">Farmer</label>
                                                                <input type="radio" id="Non-Farmer" name="AccountPurpose" value="Non-Farmer">
                                                                <label for="Non-Farmer">Non-Farmer</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Full Name<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Name, new { @class = "form-control", @readonly = "readonly" })
                                                            <span class="font11"><em>(Full Name as on your PAN CARD)</em></span>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Registration Type<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="custom_radio">
                                                                <input type="radio" id="Individual" name="RegType" value="Individual">
                                                                <label for="Individual">Individual</label>
                                                                <input type="radio" id="Organisation" name="RegType" value="Organisation">
                                                                <label for="Organisation">Organisation</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Gender<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="custom_radio">
                                                                <input type="radio" id="Male" name="Gender" value="Male">
                                                                <label for="Male">Male</label>
                                                                <input type="radio" id="Female" name="Gender" value="Female">
                                                                <label for="Female">Female</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Organisation Type<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="custom_radio">
                                                                <input type="radio" id="soleproprietorship" name="OrgType" value="soleproprietorship" />
                                                                <label for="soleproprietorship">Sole Proprietorship</label>
                                                                <br>
                                                                <input type="radio" id="Partnership" name="OrgType" value="Partnership">
                                                                <label for="Partnership">Partnership</label>
                                                                <br>
                                                                <input type="radio" id="LLP" name="OrgType" value="LLP">
                                                                <label for="LLP">LLP</label>
                                                                <br>
                                                                <input type="radio" id="privatelimited" name="OrgType" value="privatelimited">
                                                                <label for="privatelimited">Private Limited</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Date of Birth <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.DOB, new { @class = "form-control selectdate", @readonly = "readonly" })

                                                        </div>
                                                        <div class="col-md-5"><em>Date of Birth must be as per PAN CARD.</em> </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Date of Commencement <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.CommencementDate, new { @class = "form-control selectdate", @readonly = "readonly" })
                                                        </div>
                                                        <div class="col-md-5"><em>Date of Commencement must be as per ROC.</em> </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Marital Status<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="custom_radio">
                                                                <input type="radio" id="Married" name="MaritalStatus" value="Married">
                                                                <label for="Married">Married</label>
                                                                <input type="radio" id="Single" name="MaritalStatus" value="Single">
                                                                <label for="Single">Single</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Password<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Password, new { @class = "form-control", @type = "password" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Password must be minimum 6characters long, you can use number orletters of your choice.</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Confirm Password<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.ConPassword, new { @class = "form-control", @type = "password" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Confirm password should matchpassword entered above.</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Mobile No.<span class="text-danger">*</span> </label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.MobileNo, new { @class = "form-control only-numeric", @onchange = "checkMobile(this.id)", @readonly = "readonly", @maxlength = "10" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Mobile no registered here will be usedfor all official communications.</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Email Id <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.EmailId, new { @class = "form-control", @onchange = "validateEmail()", @readonly = "readonly" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Email ID registered here will be used forall official mail communications.</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Aadhar No <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.AadharNo, new { @class = "form-control only-numeric", @maxlength = "12", @onchange = "ValidateAAdhar(this.id)",@readonly = "readonly"  })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Aadhar No should not be already registered for any other User ID of LifeOne.</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Upload Aadhar Photo<span class="text-danger">*</span> </label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.CustAadharImage, new { @type = "file", @onchange = "UploadFile(this,'AadharImages')" })
                                                            @Html.HiddenFor(Model => Model.CustAadharImage, new { @Id = "HdnCustAadharImage" })
                                                            <span id="spanCustAadharImage"><a href="#" onclick="ShowAllImage('@Model.CustAadharImage')"><span id="CustAadharImagestatusval"></span></a></span>
                                                        </div>
                                                        <div class="col-md-5"><em class="font11"></em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">PAN No <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.PanNo, new { @class = "form-control Pancard", @onchange = "ValidatePAN(this.id)" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">PAN No should not be already registeredfor any other User ID of LifeOne.</em> </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">GST No<span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.GSTNo, new { @class = "form-control" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">GST No should not be alreadyregistered for any other User ID of LifeOne</em></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                            </div>
                            <div class="formtab">
                                <div class="row">
                                    <div class="col-md-12 mt-15">
                                        <div class="h3 text-center">Step 2: ADDRESS DETAILS</div>
                                    </div>
                                    <div class="col-md-12 mb-15">
                                        <fieldset>
                                            <legend>Current Address</legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Address Line 1 <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Address1, new { @class = "form-control" })
                                                        </div>
                                                        <div class="col-md-5"><em class="font11">Enter House No or name, Building Name and Sector No or Street Name</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Address Line 2</label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Address2, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Landmark</label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.LandMark, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Pin Code <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Pincode, new { @class = "form-control only-numeric", @maxlength = "6", @minlength = "6" })
                                                        </div>
                                                        <div class="col-md-5">
                                                            <button type="button" class="btn btn-info" onclick="GetDistrictCityStateDetailsByPincode()">Search</button>
                                                            <button type="button" class="btn btn-info" onclick="GetDistrictCityStateDetailsByPincode()">Change</button>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Village Name <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.VillageName, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Taluka / Tehsil / Block <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Tehsil, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">District <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <select id="DistrictName" class="form-control city-name" name="DistrictName" required onchange="BindDistrictDetail()"><option value="">--Select District--</option></select>
                                                            @Html.HiddenFor(Model => Model.DistrictId)
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">State <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.StateName, new { @class = "form-control", @readonly = "readonly" })
                                                            @Html.HiddenFor(Model => Model.StateId)
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Zone <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.Zone, new { @class = "form-control", @readonly = "readonly" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Country <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.DropDownListFor(model => model.CountryId,
                                     new List<SelectListItem> {
                                          new SelectListItem { Value = "0" , Text = "--Select--" },
                                          new SelectListItem { Value = "1" , Text = "India" }
                                       },
                                     new { @class = "form-control" })

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                            </div>
                            <div class="formtab">
                                <div class="row">
                                    <div class="col-md-12 mt-15">
                                        <div class="h3 text-center">Step 3: FAMILY DETAILS</div>
                                    </div>
                                    <div class="col-md-12 mb-15">
                                        <fieldset>
                                            <legend>SPOUSE DETAILS</legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Spouse Name <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.SpouseName, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Aadhar No <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.SpouseAadharNo, new { @class = "form-control only-numeric", @maxlength = "12", @onchange = "ValidateAAdhar(this.id)" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">PAN No</label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.SpousePanNo, new { @class = "form-control", @onchange = "ValidatePAN(this.id)" })

                                                        </div>
                                                        <div class="col-md-5"><em class="font11">(PAN Optional for User ID)</em></div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">Upload Aadhar <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.TextBoxFor(Model => Model.SpouseAadharImages, new { @type = "file", @onchange = "UploadFile(this,'AadharImages')" })
                                                            @Html.HiddenFor(Model => Model.SpouseAadharImages, new { @Id = "HdnSpouseAadharImages" })
                                                            <span id="spanSpouseAadharImages"><a onclick="ShowAllImage('@Model.SpouseAadharImages')"><span id="SpouseAadharImagesstatusval"></span></a></span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                    <div class="col-md-12 mb-15">
                                        <fieldset>
                                            <legend>CHILD DETAILS - 1</legend>

                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">Name of child : <span class="text-danger">*</span></label>
                                                <div class="col-md-4">
                                                    @Html.TextBoxFor(Model => Model.ChildName, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">Relation <span class="text-danger">*</span></label>
                                                <div class="col-md-4">
                                                    <div class="custom_radio">
                                                        <input type="radio" id="Son" name="Relation" value="Son">
                                                        <label for="Son">Son</label>
                                                        <input type="radio" id="Daughter" name="Relation" value="Daughter">
                                                        <label for="Daughter">Daughter</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">Aadhar No</label>
                                                <div class="col-md-4">
                                                    @Html.TextBoxFor(Model => Model.ChildAadhar, new { @class = "form-control only-numeric", @maxlength = "12", @onchange = "ValidateAAdhar(this.id)" })
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">PAN No</label>
                                                <div class="col-md-4">
                                                    @Html.TextBoxFor(Model => Model.ChildPanNo, new { @class = "form-control", @onchange = "ValidatePAN(this.id)" })
                                                </div>
                                                <div class="col-md-5"><em class="font11">(For age above 18)</em></div>
                                            </div>
                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">Upload Aadhar</label>
                                                <div class="col-md-4">
                                                    @Html.TextBoxFor(Model => Model.ChildAadharImage, new { @type = "file", @onchange = "UploadFile(this,'AadharImages')" })
                                                    @Html.HiddenFor(Model => Model.ChildAadharImage, new { @Id = "HdnChildAadharImage" })
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <label class="col-md-3 control-label">&nbsp;</label>
                                                <div class="col-md-4">
                                                    <button type="button" class="btn btn-info" id="addrow">+ Add Child</button>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <div id="divMultipleFamilyTable"></div>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                            </div>
                            <div class="formtab">
                                <div class="row">
                                    <div class="col-md-12 mt-15">
                                        <div class="h3 text-center">Step 4: FARM DETAILS</div>
                                    </div>
                                    <div class="col-md-12 mb-15">
                                        <fieldset>
                                            <legend>Land & Agriculture Info</legend>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">How many acre total farm land ? <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            <div class="row">
                                                                <div class="col-md-8">
                                                                    @Html.TextBoxFor(Model => Model.TotalLand, new { @class = "form-control only-numeric" })
                                                                </div>
                                                                <div class="col-md-4">
                                                                    <select class="form-control">
                                                                        <option>Acre</option>
                                                                    </select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">FRUIT CROPS <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.DropDownListFor(model => model.FruitImages,
                                     new List<SelectListItem> {
                                          new SelectListItem { Value = "0" , Text = "--Select--" },
                                          new SelectListItem { Value = "Option1" , Text = "Option1" },
                                          new SelectListItem { Value = "Option2" , Text = "Option2" },
                                          new SelectListItem { Value = "Option3" , Text = "Option3" },
                                          new SelectListItem { Value = "Option4" , Text = "Option4" },
                                       },
                                     new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">FRUIT VEGETABLE CROPS <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
                                                            @Html.DropDownListFor(model => model.FruitVegetablesImages,
                   new List<SelectListItem> {
                         new SelectListItem { Value = "0" , Text = "--Select--" },
                                          new SelectListItem { Value = "Option1" , Text = "Option1" },
                                          new SelectListItem { Value = "Option2" , Text = "Option2" },
                                          new SelectListItem { Value = "Option3" , Text = "Option3" },
                                          new SelectListItem { Value = "Option4" , Text = "Option4" },
                     },
                   new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="row form-group">
                                                        <label class="col-md-3 control-label">LEAF VEGETABLE CROPS <span class="text-danger">*</span></label>
                                                        <div class="col-md-4">
